{
  "version": 2,
  "builds": [
    { "src": "*.html", "use": "@vercel/static" },
    { "src": "admin/*.html", "use": "@vercel/static" },
    { "src": "admin/*.yml", "use": "@vercel/static" },
    { "src": "images/**", "use": "@vercel/static" }
  ],
  "rewrites": [
    {
      "source": "/admin",
      "destination": "/admin/index.html"
    }
  ]
}
